{
    "manifest_version": 2,
    "name": "Website Tracker",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "tabs"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js":["popup.js"]
      }
    ]
  }
  